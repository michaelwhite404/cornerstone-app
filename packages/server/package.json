{
  "name": "@cornerstone/server",
  "version": "2.4.1",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc ; tsc-alias ; rm -rf ./dist/graphql/typedefs && cp -r ./src/graphql/typedefs ./dist/graphql/typedefs",
    "watch": "concurrently -n 'tsc,alias' -c 'yellow,magenta' --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "server": "nodemon dist/server.js",
    "dev": "concurrently -n 'watch,server' -c 'yellow,green' --kill-others \"npm run watch\" \"npm run server\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/lib-storage": "^3.990.0",
    "@graphql-tools/schema": "^8.2.0",
