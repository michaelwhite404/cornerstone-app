    "bcryptjs": "^3.0.3",
    "capitalize": "^2.0.3",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.5",
    "csv-stringify": "^6.0.4",
    "date-fns": "^4.1.0",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-subdomain": "^1.0.5",
    "google-auth-library": "^7.6.2",
    "googleapis": "^87.0.0",
    "graphql": "^15.5.3",
    "graphql-http": "^1.22.4",
    "html-to-text": "^9.0.2",
    "jsonwebtoken": "^9.0.3",
    "lodash.isplainobject": "^4.0.6",
    "moment": "^2.29.1",
    "mongoose": "^8.23.0",
    "mongoose-autopopulate": "^1.2.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nanoid": "^3.2.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^8.0.1",
    "nodemon": "^3.1.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfmake": "^0.2.4",
    "pluralize": "^8.0.0",
    "pug": "^3.0.2",
    "qs": "^6.14.2",
    "slugify": "^1.6.0",
    "socket.io": "^4.5.0",
    "tsc-alias": "^1.6.11",
    "validator": "^13.7.0",
    "xss": "^1.0.15"
